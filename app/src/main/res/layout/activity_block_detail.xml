                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_centerHorizontal="true"
                                                android:text="-"
                                                android:textColor="@color/black_shine"
                                                android:textSize="@dimen/text_size_12sp" />

                                            <TextView
                                                android:layout_width="wrap_content"

                                                android:layout_height="wrap_content"
                                                android:layout_alignParentRight="true"
                                                android:text="-"
                                                android:textColor="@color/black_shine"
                                                android:textSize="@dimen/text_size_12sp"
                                                android:visibility="gone" />
                                        </RelativeLayout>

                                    </LinearLayout>
                                </RelativeLayout>
                            </android.support.v7.widget.CardView>
                        </LinearLayout>

                        <android.support.v7.widget.RecyclerView
                            android:id="@+id/blockList"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"

                            android:scrollbars="vertical"
                            tools:listitem="@layout/block_list_item" />

                    </LinearLayout>
                </RelativeLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/syncButtonCardView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible"
                card_view:cardBackgroundColor="@color/white"
                card_view:cardCornerRadius="5dp"
                card_view:cardElevation="5dp"
                card_view:cardUseCompatPadding="true">

                <RelativeLayout
                    android:id="@+id/syncRelativeLayout"
                    style="@style/buttonGrey"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_margin="10dp"
                    android:orientation="horizontal"
                    android:weightSum="1">

                    <Button
                        android:id="@+id/syncAadhaarCollectedBT"
                        style="@style/buttonGrey"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_centerHorizontal="true"
                        android:layout_margin="5dp"
                        android:text="@string/syncHousehold" />
                    />

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="15dp"
                        android:background="@drawable/arrow_righttt" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"></LinearLayout>

                <Button
                    android:id="@+id/syncEBDataBT"
                    style="@style/buttonGrey"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_margin="5dp"
                    android:text="@string/SyncNonAdhar"
                    android:visibility="gone" />

                <Button
                    android:id="@+id/resetDataBT"
                    style="@style/buttonGrey"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_margin="5dp"
                    android:text="@string/resetEnumBlock"
                    android:visibility="gone" />
            </android.support.v7.widget.CardView>

